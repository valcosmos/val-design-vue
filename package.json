{
  "name": "valtify",
  "private": false,
  "version": "0.1.3",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/scripts/entry.d.ts",
  "license": "MIT",
  "description": "A simple UI component library for Vue3",
  "author": "Cupid Valentine",
  "keywords": [
    "valtify",
    "front-end",
    "CSS",
    "Sass",
    "Vue3",
    "Component",
    "UI",
    "TypeScript"
  ],
  "homepage": "https://valcosmos.github.io/valtify/",
  "repository": {
    "type": "git",
    "url": "https://github.com/valcosmos/valtify.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.ts,.tsx,.vue",
    "format": "prettier --write ./**/*.{vue,ts,tsx,md,scss,json}",
    "prepare": "husky install",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "test": "vitest",
    "build:component": "node ./scripts/build.js",
    "prepublishOnly": "pnpm test && pnpm build:component"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.5.0",
    "@heroicons/vue": "^2.0.18",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@testing-library/vue": "^7.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.3",
    "dirname-filename-esm": "^1.1.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.15.1",
    "fs-extra": "^11.1.1",
    "happy-dom": "^10.5.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "rollup-plugin-typescript2": "^0.35.0",
    "sass": "^1.64.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitepress": "1.0.0-beta.6",
    "vitepress-theme-demoblock": "^3.0.3",
    "vitest": "^0.33.0",
    "vue-loader": "^17.2.2",
    "vue-tsc": "^1.8.8"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css,scss,md}": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
